{
  "$schema": "../../chain.schema.json",
  "chain_name": "ojotestnet",
  "status": "live",
  "network_type": "testnet",
  "pretty_name": "Ojo Network Testnet",
  "chain_id": "ojo-devnet-1",
  "bech32_prefix": "ojo",
  "daemon_name": "ojod",
  "node_home": "$HOME/.ojo",
  "key_algos": [
    "secp256k1"
  ],
  "slip44": 118,
  "fees": {
    "fee_tokens": [
      {
        "denom": "uojo",
        "fixed_min_gas_price": 0,
        "low_gas_price": 0,
        "average_gas_price": 0,
        "high_gas_price": 0.025
      }
    ]
  },
  "staking": {
    "staking_tokens": [
      {
        "denom": "uojo"
      }
    ]
  },
  "codebase": {
    "git_repo": "https://github.com/ojo-network/ojo",
    "recommended_version": "v0.2.1",
    "compatible_versions": [
      "v0.2.1"
    ],
    "binaries": {
      "darwin/amd64": "https://github.com/ojo-network/ojo/releases/download/v0.2.1/ojod-v0.2.1-darwin-amd64.tar.gz",
      "darwin/arm64": "https://github.com/ojo-network/ojo/releases/download/v0.2.1/ojod-v0.2.1-darwin-arm64.tar.gz",
      "linux/amd64": "https://github.com/ojo-network/ojo/releases/download/v0.2.1/ojod-v0.2.1-linux-amd64.tar.gz",
      "linux/arm64": "https://github.com/ojo-network/ojo/releases/download/v0.2.1/ojod-v0.2.1-linux-arm64.tar.gz"
    },
    "cosmos_sdk_version": "0.47.3",
    "consensus": {
      "type": "cometbft",
      "version": "0.37.2"
    },
    "ibc_go_version": "7.2.1",
    "ics_enabled": [
      "ics20-1"
    ]
  },
  "apis": {
    "rpc": [
      {
        "address": "https://rpc-1.ojo.nodes.guru/",
        "provider": "Ojo Network"
      }
    ],
    "rest": [
      {
        "address": "https://api-1.ojo.nodes.guru/",
        "provider": "Ojo Network"
      }
    ]
  },
  "explorers": [
    {
      "kind": "Ojo Network",
      "url": "https://sauron.ojo.network/ojo-devnet-1",
      "tx_page": "https://sauron.ojo.network/ojo-devnet-1/tx/${txHash}"
    }
  ],
  "logo_URIs": {
    "png": "https://raw.githubusercontent.com/cosmos/chain-registry/master/testnets/ojotestnet/images/ojo.png"
  },
  "images": [
    {
      "png": "https://raw.githubusercontent.com/cosmos/chain-registry/master/testnets/ojotestnet/images/ojo.png"
    }
  ]
}